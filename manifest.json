{
  "manifest_version": 2,

  "name": "Vk video minimizer",
  "description": "This extension automatically minimizes video that is out of scope",
  "version": "1.0",

  "icons": { "16": "/img/icon16.png",
             "48": "/img/icon48.png",
             "128": "/img/icon128.png" },
             
  "content_scripts": [{
    "matches": ["https://vk.com/*"],
    "js": ["/js/main.js"] 
  }] 
}